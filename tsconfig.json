{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "sourceMap": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "allowJs": true,
    "jsx": "preserve",
    "lib": [
      "esnext"
    ]
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}
